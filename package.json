  "description": "muonline website server-side typescript",
  "main": "build/app.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "prebuild": "yarn lint",
    "build": "tsc",
    "pretest": "yarn build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prebundle": "yarn lint",
    "bundle": "env NODE_ENV=production webpack",
    "prestart": "yarn bundle",
    "start": "node .",
    "predev": "yarn lint",
    "dev": "env NODE_ENV=development webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dea7h-cyber/NyxWeb-server-ts.git"
  },
  "keywords": [
    "nyxmu",
    "nyxweb",
    "muonline",
    "typescript",
    "express",
    "sequelize"
  ],
  "author": "Dea7h",
  "bugs": {
    "url": "https://github.com/Dea7h-cyber/NyxWeb-server-ts/issues"
  },
  "homepage": "https://github.com/Dea7h-cyber/NyxWeb-server-ts#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "tedious": "^8.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.7",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.7",
    "@types/validator": "^12.0.1",
    "chai": "^4.2.0",
    "jest": "^25.1.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "nodemon-webpack-plugin": "^4.2.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
