{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-angle-bracket-type-assertion": false,
    "callable-types": false,
    "class-name": false,
    "variable-name": false,
    "quotemark": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "interface-name": false,
    "member-access": false
  },
  "rulesDirectory": []
}
