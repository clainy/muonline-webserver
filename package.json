{
  "name": "nyxweb-server-ts",
  "version": "1.0.0",
  "description": "muonline website server-side typescript",
  "main": "dist/bundle.js",
  "scripts": {
    "clear": "rm -r dist/* || true",
    "prebuild": "yarn clear && tslint -c tslint.json -p tsconfig.json",
    "build": "webpack",
    "postbuild": "yarn test",
    "prestart": "yarn build",
    "start": "node .",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "dev": "yarn clear && tsc",
    "postdev": "node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dea7h-cyber/NyxWeb-server-ts.git"
  },
  "keywords": [
    "nyxmu",
    "nyxweb",
    "muonline",
    "typescript",
    "express",
    "sequelize"
  ],
  "author": "Dea7h",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dea7h-cyber/NyxWeb-server-ts/issues"
  },
  "homepage": "https://github.com/Dea7h-cyber/NyxWeb-server-ts#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "sequelize": "^5.21.3",
    "tedious": "^8.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.7",
    "@types/sequelize": "^4.28.8",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
