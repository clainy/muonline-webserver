{
  "name": "nyxweb-server-ts",
  "version": "1.0.0",
  "description": "muonline website server-side typescript",
  "main": "build/app.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "predev": "yarn lint && yarn test",
    "dev": "env NODE_ENV=development nodemon src/app.ts",
    "start": "env NODE_ENV=production tsc && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dea7h-cyber/NyxWeb-server-ts.git"
  },
  "keywords": [
    "nyxmu",
    "nyxweb",
    "muonline",
    "typescript",
    "express",
    "sequelize"
  ],
  "author": "Dea7h",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dea7h-cyber/NyxWeb-server-ts/issues"
  },
  "homepage": "https://github.com/Dea7h-cyber/NyxWeb-server-ts#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "tedious": "^8.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/chai": "^4.2.7",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.7",
    "@types/validator": "^12.0.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
}
